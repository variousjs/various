{
  "name": "@variousjs/various",
  "version": "0.4.1",
  "description": "RequireJS(AMD) + React",
  "files": [
    "dist",
    "types"
  ],
  "unpkg": "dist/index.js",
  "publishConfig": {
    "access": "public"
  },
  "types": "types/index.d.ts",
  "scripts": {
    "lint": "tsc --noemit && eslint . --ext .ts,.tsx,.js",
    "start": "webpack --config webpack/components.js --progress & webpack serve --config webpack/index.js --progress",
    "build": "NODE_ENV=production webpack --progress --config webpack/components.js && NODE_ENV=production webpack --config webpack/index.js --progress",
    "postbuild": "cp -R dist/ docs/dist/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/variousjs/various.git"
  },
  "keywords": [
    "requirejs",
    "amd",
    "react"
  ],
  "author": "LoeiFy <LoeiFy@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/variousjs/various/issues"
  },
  "homepage": "https://github.com/variousjs/various#readme",
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@babel/runtime-corejs3": "^7.14.8",
    "@types/react": "^17.0.27",
    "@types/react-dom": "^17.0.9",
    "@types/react-router-dom": "^5.3.1",
    "@types/requirejs": "^2.1.33",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.31.0",
    "babel-loader": "^8.2.2",
    "eslint": "^7.31.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "typescript": "^4.4.2",
    "webpack": "^5.60.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.3.1"
  },
  "dependencies": {
    "nycticorax": "^2.0.6",
    "react-router-dom": "^5.3.0"
  }
}
