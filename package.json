{
  "name": "@fratercula/humpback",
  "version": "1.0.0",
  "description": "RequireJS(AMD) + React",
  "files": [
    "dist",
    "types"
  ],
  "publishConfig": {
    "access": "public"
  },
  "types": "types/index.d.ts",
  "scripts": {
    "lint": "eslint . --ext .ts,.tsx,.js",
    "build": "ENTRY=src/index.ts webpack --progress && cp -R dist/ docs/dist/",
    "a": "ENTRY=demo/components/a.tsx LIBRARY=a webpack --progress",
    "b": "ENTRY=demo/components/b.tsx LIBRARY=b webpack --progress",
    "c": "ENTRY=demo/components/c.tsx LIBRARY=c webpack --progress",
    "d": "ENTRY=demo/components/d.tsx LIBRARY=d webpack --progress",
    "e": "ENTRY=demo/components/e.tsx LIBRARY=e webpack --progress",
    "f": "ENTRY=demo/components/f.tsx LIBRARY=f webpack --progress",
    "hooks": "ENTRY=demo/components/hooks.tsx LIBRARY=hooks webpack --progress",
    "entry": "ENTRY=demo/index.ts LIBRARY=entry webpack --progress"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fratercula/humpback.git"
  },
  "keywords": [
    "requirejs",
    "amd",
    "react"
  ],
  "author": "LoeiFy <LoeiFy@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fratercula/humpback/issues"
  },
  "homepage": "https://github.com/fratercula/humpback#readme",
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@babel/runtime-corejs3": "^7.14.8",
    "@types/react-dom": "^17.0.9",
    "@types/requirejs": "^2.1.33",
    "@typescript-eslint/eslint-plugin": "^4.28.4",
    "@typescript-eslint/parser": "^4.28.4",
    "babel-loader": "^8.2.2",
    "css-loader": "^6.1.0",
    "eslint": "^7.31.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "react": "^17.0.2",
    "style-loader": "^3.1.0",
    "typescript": "^4.3.5",
    "webpack": "^5.45.1",
    "webpack-cli": "^4.7.2"
  },
  "dependencies": {
    "@types/react": "^17.0.15",
    "@types/react-router-dom": "^5.1.8",
    "nycticorax": "^2.0.6"
  }
}
